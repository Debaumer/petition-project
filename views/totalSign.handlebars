<h1>{{cause}}</h1>
<h2>Meet your fellow supporters</h2>
<div id="supporters">
  {{#supporter}}
  <div class="supporter">
        <h3>{{firstName}} {{lastName}}</h3>
        <h5>{{age}}</h5>
        <h5>{{city}}</h5>
        <h5>{{homepage}}</h5>
        {{#if signature}}
            <h5><em>Signed:</em></h5>
            <img class="signature" src="{{signature}}">
        {{/if}}
    </div>
  {{/supporter}}
</div>
